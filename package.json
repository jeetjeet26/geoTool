{
  "name": "llm-serp-tracker",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": "20.11.1"
  },
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm -r build",
    "heroku-postbuild": "npm install -g pnpm@9.0.0 && pnpm install && pnpm --filter @geo/db prisma generate && pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "format": "pnpm -r format",
    "run:once": "pnpm --filter worker run once",
    "seed": "pnpm --filter @geo/db seed"
  }
}
